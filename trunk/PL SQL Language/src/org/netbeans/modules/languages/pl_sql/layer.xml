<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="pl_sql">
                <!--file name="language.nbs" url="PL_SQL.nbs">
                    <attr name="icon" stringvalue="org/netbeans/modules/languages/pl_sql/images/PL_SQL.png"/>
                </file-->
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.languages.pl_sql.Bundle"/>
                <file name="language.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.languages.pl_sql.lexer.PL_SQLTokenId.getLanguage"/>
